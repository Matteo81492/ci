pipeline {
  agent any
  stages {
    stage('Build') {
      steps {
        echo 'In the Build stage'
      }
    }

    stage('Test') {
      steps {
        echo 'In the Test stage'
      }
    }

    stage('Deploy') {
      steps {
        echo 'In the Deploy stage'
        sleep 10
      }
    }

  }
}
